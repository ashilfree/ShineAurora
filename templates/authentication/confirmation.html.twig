{% extends 'base.html.twig' %}

{% block body %}

    <div class="breadcrumb-area" style="background-image: url({{ vich_uploader_asset(banner, 'imageFile') | imagine_filter('Extra_large') }});background-repeat:no-repeat;background-position:center;">
        <div class="container">
            <div class="breadcrumb-content">
                <h2>Confirmation</h2>
                <ul>
                    <li><a href="{{ path('home', {locale:'en'}) }}">Home</a></li>
                    <li class="active">Confirmation</li>
                </ul>
            </div>
        </div>
    </div>
    <!-- Hiraola's Breadcrumb Area End Here -->
    <div style="padding-top: 60px">
        <div class="container">
            <div>
                <ul>
                    <li class="text-center" style="color: #595959;font-size: 25px;"><a href="{{ path('home', { locale: 'en' }) }}">Home</a> / Confirmation</li>
                </ul>
            </div>
        </div>
    </div>
    <!-- Begin Hiraola's Error 404 Page Area -->
    <div class="error404-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 ml-auto mr-auto text-center">
                    <div class="search-error-wrapper">
                        <h1>Confirmation</h1>
                        <h2>Thank you!</h2>
                        <p class="short_desc">You can now login with your credential</p>
                        <div class="hiraola-btn-ps_center"></div>
                        <a href="{{ path('login', { locale: 'en' }) }}" class="hiraola-error_btn">go to Login Page</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Hiraola's Error 404 Page End Here -->

{% endblock %}



