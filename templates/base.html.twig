                    </div>
                    <nav class="offcanvas-navigation">
                        <ul class="mobile-menu">
                            <li><a href="{{ path('home', { locale: 'en' }) }}"><span class="mm-text">Home</span></a>

                            </li>
                            <li>
                                <a href="{{ path('products', { locale: 'en' }) }}">
                                    <span class="mm-text">Shop</span>
                                </a>
                            </li>
                            <li>
                                <a href="{{ path('about.us', { locale: 'en' }) }}">
                                    <span class="mm-text">About Us</span>
                                </a>
                            </li>
                            <li>
                                <a href="{{ path('contact.us', { locale: 'en' }) }}">
                                    <span class="mm-text">Contact</span>
                                </a>
                            </li>
                        </ul>
                    </nav>
                    <nav class="offcanvas-navigation user-setting_area">
                        <ul class="mobile-menu">
                            {% if app.user %}
                                <li>
                                    <a href="{{ path('account', { locale: 'en' }) }}">
                                        <span class="mm-text">My Account</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="{{ path('logout', { locale: 'en' }) }}">
                                        <span class="mm-text">Logout</span>
                                    </a>
                                </li>
                            {% else %}
                                <li>
                                    <a href="{{ path('login', { locale: 'en' }) }}">
                                        <span class="mm-text">Login</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="{{ path('register', { locale: 'en' }) }}">
                                        <span class="mm-text">Register</span>
                                    </a>
                                </li>
                            {% endif %}
                            <li>
                                <a href="{{ path('home', { locale: 'ar' }) }}">
                                    <span class="mm-text">Arabic</span>
                                </a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
