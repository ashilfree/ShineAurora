        <div class="offcanvas-minicart_wrapper" id="miniCart">
            {% set total = null %}
            <div class="offcanvas-menu-inner">
                <a href="#" class="btn-close"><i class="ion-android-close"></i></a>
                <div class="minicart-content">
                    <div class="minicart-heading">
                        <h4>Shopping Cart</h4>
                    </div>
                    {% if cart %}
                        <ul class="minicart-list">
                            {% for cartItem in cart %}
                                <li class="minicart-product">
                                    <a class="product-item_remove"
                                       href="{{ path('delete.cart', {id: cartItem.catalog.id, route:page}) }}"><i
                                                class="ion-android-close"></i></a>
                                    <div class="product-item_img">
                                        <img src="{{ vich_uploader_asset(cartItem.catalog.product.images.first, 'imageFile') }}"
                                             alt="{{ cartItem.catalog.product.name }}">
                                    </div>
                                    <div class="product-item_content">
                                        <a class="product-item_title"
                                           href="{{ path('product', {'slug': cartItem.catalog.product.slug }) }}">{{ cartItem.catalog.product.name|upper }}</a>
                                        <span class="product-item_quantity"
                                              style="display: flex;justify-content:space-between; ">
                                                <a href="javascript:void(0)" class="cart-single-color"
                                                   data-swatch-color="red">
                                                    <span style="background-color: {{ cartItem.catalog.color }};display: block; width:100%;height: 100%"></span>
                                                </a>
                                                Size: {{ cartItem.catalog.size }}
                                        </span>
                                        <span class="product-item_quantity">
                                    {% if cartItem.catalog.product.discountPrice != 0 %}
                                        {{ cartItem.quantity }} x {{ (cartItem.catalog.product.discountPrice / 100) |number_format(2, '.', ',') }} {{ currency }}
                                    {% else %}
                                        {{ cartItem.quantity }} x {{ (cartItem.catalog.product.price / 100) |number_format(2, '.', ',') }} {{ currency }}
                                    {% endif %}
                                </span>
                                    </div>
                                </li>
                                {% if cartItem.catalog.product.discountPrice != 0 %}
                                    {% set total = total + (cartItem.quantity * cartItem.catalog.product.discountPrice) %}
                                {% else %}
                                    {% set total = total + (cartItem.quantity * cartItem.catalog.product.price) %}
                                {% endif %}
                            {% endfor %}
                        </ul>
                    {% else %}
                        <img class="img-responsive" src="{{ '/media/images/waiting.png' }}" width=240 alt="ICON-PAY">

                    {% endif %}
                </div>
                <div class="minicart-item_total">
                    <span>Subtotal</span>
                    <span class="ammount">
                        {% if cart %}
                            {{ (total / 100)|number_format(2) }} {{ currency }}
                        {% else %}
                            00.0 {{ currency }}
                        {% endif %}
                    </span>
                </div>
                <div class="minicart-btn_area">
                    <a href="{{ path('cart') }}" class="hiraola-btn hiraola-btn_dark hiraola-btn_fullwidth">View
                        Cart</a>
                </div>
                <div class="minicart-btn_area">
                    <a {% if cart|length != 0 %} href="{{ path('remove.cart', {route: page}) }}" {% endif %}
                            class="hiraola-btn hiraola-btn_dark hiraola-btn_fullwidth"
                            {% if cart|length == 0 %} disabled="true" {% endif %}>Clear List</a>
                </div>
            </div>
        </div>
        <div class="mobile-menu_wrapper" id="mobileMenu">
            <div class="offcanvas-menu-inner">
                <div class="container">
                    <a href="#" class="btn-close"><i class="ion-android-close"></i></a>
                    <div class="offcanvas-inner_search">
                        <form action="#" class="hm-searchbox">
