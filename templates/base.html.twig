        {% if popup.isShow %}
            {% if newsletter is defined and not newsletter %}
                <div class="popup_wrapper">
                    <div class="test" style="background-image: url({{ vich_uploader_asset(popup, 'imageFile') }});">
                        <span class="popup_off"><i class="ion-android-close"></i></span>
                        <div class="subscribe_area">
                            <h2>Sign Up Newsletter</h2>
                            <p>Subscribe to the our store mailing list to receive updates on new arrivals, special
                                offers and other discount information.</p>
                            <div class="subscribe-form-group">
                                {{ form_start(newsletterForm, {'attr': {'class': 'subscribe-form'}}) }}
                                {{ form_widget(newsletterForm.email, {'attr': {'placeholder':'Enter your email address', 'autocomplete':'off'}}) }}
                                <button type="submit">subscribe</button>
